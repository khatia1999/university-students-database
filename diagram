TABLE uni{
   uni_id INT [pk]
   uni_name VARCHAR
   phone VARCHAR
   email VARCHAR
   street VARCHAR
   city VARCHAR
   country VARCHAR
}

TABLE lecturer{
  lecturer_id INT [pk]
  lecturer_name VARCHAR
  lecturer_lastname VARCHAR
  birth_date VARCHAR
  gender VARCHAR
  address VARCHAR
  second_address VARCHAR
  flat_number INT
  email VARCHAR
  account_number VARCHAR
  rank VARCHAR
 }
 

 
 
TABLE students{
  students_id  INT [pk]
  students_name VARCHAR
  students_lastname VARCHAR
  students_status VARCHAR
  birth_date VARCHAR
  gender VARCHAR
  address VARCHAR
  second_address VARCHAR
  flat_number INT
  email VARCHAR
  students_transaction VARCHAR
}

TABLE assistant_Manager{
    assistant_Manager_id INT [pk]
    assistant_Manager_name VARCHAR
    assistant_Manager_lastname VARCHAR
    gender VARCHAR
    address VARCHAR
    email VARCHAR
}




TABLE courses{
    courses_id VARCHAR
    courses_name VARCHAR
    courses_price VARCHAR
    duration VARCHAR
}

TABLE subjects{
    subjects_id VARCHAR
    subjects_name VARCHAR
    datetime VARCHAR

}  

Ref: subjects.subjects_id > courses.courses_id



TABLE transaction {
      trans_id INT [pk]
      amount VARCHAR
      trans_date INT
      students_id [FK]
}



Ref: students.students_id > courses.courses_id
Ref: lecturer.lecturer_id > subjects.subjects_id
Ref: transaction.students_id > students.students_id
Ref: assistant_Manager.assistant_Manager_id > uni.uni_id


